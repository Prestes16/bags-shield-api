{
    "rewrites":  [
                     {
                         "source":  "/api/:path*",
                         "destination":  "/api/:path*"
                     },
                     {
                         "source":  "/",
                         "destination":  "/public/index.html"
                     },
                     {
                         "source":  "/:path*",
                         "destination":  "/public/:path*"
                     }
                 ],
    "routes":  [
                   {
                       "src":  "/api/token/([^/]+)/creators",
                       "dest":  "/api/token/creators?mint="
                   }
               ]
}
