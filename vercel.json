{
  "version": 2,
  "functions": {
    "api/**/*.ts": { "runtime": "nodejs18.x" }
  },
  "rewrites": [
    { "source": "/api/health", "destination": "/api/router?path=/api/health" },
    { "source": "/api/token/:mint/creators", "destination": "/api/router?path=/api/token/:mint/creators&mint=:mint" },
    { "source": "/api/token/:mint/lifetime-fees", "destination": "/api/router?path=/api/token/:mint/lifetime-fees&mint=:mint" }
  ]
}
