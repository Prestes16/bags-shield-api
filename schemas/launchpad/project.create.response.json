{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "launchpad.project.create.response",
  "title": "Launchpad - Create Project Response",
  "type": "object",
  "additionalProperties": false,
  "required": ["mint", "configKey", "tx", "createdAt", "requestId"],
  "properties": {
    "mint": {
      "type": "string",
      "minLength": 32,
      "maxLength": 44,
      "pattern": "^[1-9A-HJ-NP-Za-km-z]{32,44}$",
      "description": "Mint address do token criado"
    },
    "configKey": {
      "type": "string",
      "minLength": 32,
      "maxLength": 44,
      "pattern": "^[1-9A-HJ-NP-Za-km-z]{32,44}$",
      "description": "Chave de configuração do Bags"
    },
    "tx": {
      "type": ["string", "null"],
      "description": "Transação base64 para assinar (ou null se não necessário)"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp da criação (ISO 8601)"
    },
    "requestId": {
      "type": "string",
      "format": "uuid",
      "description": "Request ID para rastreamento"
    }
  }
}
